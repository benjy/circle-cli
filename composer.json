{
  "name": "code-drop/circle-cli",
  "description": "A CLI utility built with Symfony Console to query Circle CI projects.",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Ben Dougherty",
      "email": "ben@dougherty.net.au"
    }
  ],
  "require": {
    "php": ">=5.4.5",
    "symfony/console": "2.6.*",
    "symfony/yaml": "2.6.*",
    "guzzlehttp/guzzle": "~5.0",
    "pimple/pimple": "~3.0",
    "symfony/event-dispatcher": "2.6.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mikey179/vfsStream": "1.5.0",
    "sami/sami": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Circle\\": "src/",
      "Circle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "bin": [
    "circle"
  ],
  "config": {
    "bin-dir": "bin"
  }
}
