{
  "name": "codedrop/circle-cli",
  "description": "A CLI utility built with Symfony Console to query Circle CI projects.",
  "authors": [
    {
      "name": "Ben Dougherty",
      "email": "ben@dougherty.net.au"
    }
  ],
  "require": {
    "php": ">=5.4.5",
    "symfony/console": "2.6.*",
    "symfony/yaml": "2.6.*",
    "guzzlehttp/guzzle": "~5.0",
    "pimple/pimple": "~3.0",
    "phpunit/phpunit": "4.6.*",
    "mikey179/vfsStream": "1.5.0"
  },
  "autoload": {
    "psr-4": {
      "Circle\\": "src/",
      "Circle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "bin": [
    "circle"
  ],
  "config": {
    "bin-dir": "bin"
  }
}
